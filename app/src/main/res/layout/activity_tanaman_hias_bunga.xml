<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.notifications.TanamanHiasBunga.TanamanHiasBungaActivity"
    android:background="@color/warnaBackgroundApp"
    >

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/carter_one"
        android:text="Tanaman Hias Bunga"
        android:textColor="@color/warnaTulisan"
        android:textSize="25dp"
        tools:ignore="MissingConstraints"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="3dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="70dp"
        android:id="@+id/kategori_tanaman"
        />


    <ImageView
        android:id="@+id/back"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/back"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="20dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/anggrek"
        android:id="@+id/BungaEncyclopedia1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="400dp"
        tools:ignore="MissingConstraints"
        android:onClick="anggrek"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/BungaEncyclopedia1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:fontFamily="@font/carter_one"
        android:textSize="10dp"
        android:textColor="@color/warnaTulisan"
        android:text="Bunga Anggrek"
        android:id="@+id/Bunga1"
        android:onClick="anggrek"
        />

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/bunga_mawar"
        android:id="@+id/BungaEncyclopedia2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="250dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="400dp"
        tools:ignore="MissingConstraints"
        android:onClick="mawar"
        />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/BungaEncyclopedia2"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="250dp"
        android:fontFamily="@font/carter_one"
        android:textSize="10dp"
        android:textColor="@color/warnaTulisan"
        android:text="Bunga Mawar"
        android:id="@+id/Bunga2"
        android:onClick="mawar"
        />

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/bunga_melati"
        android:id="@+id/BungaEncyclopedia3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="250dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="400dp"
        tools:ignore="MissingConstraints"
        android:onClick="melati"

        />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/BungaEncyclopedia3"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:fontFamily="@font/carter_one"
        android:textSize="10dp"
        android:textColor="@color/warnaTulisan"
        android:text="Bunga Melati"
        android:layout_marginLeft="250dp"
        android:id="@+id/Bunga3"
        android:onClick="melati"
        />

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/kembang_sepatu"
        android:id="@+id/BungaEncyclopedia4"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="250dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="400dp"
        tools:ignore="MissingConstraints"
        app:layout_constraintTop_toBottomOf="@+id/Bunga1"
        android:onClick="kembangSepatu"
        />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/BungaEncyclopedia4"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="250dp"
        android:fontFamily="@font/carter_one"
        android:textSize="10dp"
        android:textColor="@color/warnaTulisan"
        android:text="Kembang Sepatu"
        android:id="@+id/Bunga4"
        android:onClick="kembangSepatu"

        />

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/bunga_kamboja"
        android:id="@+id/BungaEncyclopedia5"
        app:layout_constraintTop_toBottomOf="@id/Bunga1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="400dp"
        tools:ignore="MissingConstraints"
        android:onClick="kamboja"
        />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/BungaEncyclopedia5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:fontFamily="@font/carter_one"
        android:textSize="10dp"
        android:textColor="@color/warnaTulisan"
        android:text="Bunga Kamboja"
        android:id="@+id/Bunga5"
        android:onClick="kamboja"
        />


</androidx.constraintlayout.widget.ConstraintLayout>